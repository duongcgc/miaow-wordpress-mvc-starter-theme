<div class="row block-content block-gmaps grid-x grid-padding-x">
    <div class="cell small-12 large-10 medium-12 large-offset-1">
        {% if content_block.title %}
            <h2 class="text-center block-title">{{content_block.title}}</h2>
        {% endif %}

        {% if content_block.map_points is not empty %}
            <div id="gmaps_{{key}}" class="js-gmaps gmaps"></div>
            <script type="text/javascript">
                var points_gmaps_{{key}} = {{fn('json_encode', content_block.map_points)}};
            </script>
        {% endif %}
    </div>
</div>
